{
    "standard_ingress": true,
    "cardinality": "OnePerCluster",
    "node_selector_paths": [
        [
            "postgresql",
            "primary",
            "nodeSelector"
        ],
        [
            "postgresql",
            "readReplicas",
            "nodeSelector"
        ]
    ],
    "tolerations_paths": [
        [
            "postgresql",
            "primary",
            "tolerations"
        ],
        [
            "postgresql",
            "readReplicas",
            "tolerations"
        ]
    ]
}